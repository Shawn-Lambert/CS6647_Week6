   "source": [
    "1. The starting state is Rest. Calculate the probability of possible activity on the 15th day. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "All state transition probabilities add up. Continue.\n",
      "Start state: Rest\n",
      "Possible states: ['Rest', 'Exercise', 'Rest', 'Exercise', 'Exercise', 'Exercise', 'Exercise', 'Sweets', 'Exercise', 'Sweets', 'Exercise', 'Exercise', 'Rest', 'Sweets', 'Exercise', 'Exercise']\n",
      "End state after 15 days: Exercise\n",
      "Probability of the possible sequence of states: 2.7782999999999997e-06\n"
     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "import random as rm\n",
    "\n",
    "# All states\n",
    "states = [\"Rest\",\"Sweets\",\"Exercise\"]\n",
    "\n",
    "# Matrix of state transitions\n",
    "transitionName = [[\"RR\",\"RE\",\"RS\"],[\"ER\",\"EE\",\"ES\"],[\"SR\",\"SE\",\"SS\"]]\n",
    "\n",
    "# Probabilities of state transitions\n",
    "transitionMatrix = [[0.2,0.6,0.2],[0.1,0.6,0.3],[0.2,0.7,0.1]]\n",
    "\n",
    "if sum(transitionMatrix[0])+sum(transitionMatrix[1])+sum(transitionMatrix[1]) != 3:\n",
    "    print(\"State transitions do not add up to 1 per state for total of 3. Check weighted edges for transition matrix.\")\n",
    "else: print(\"All state transition probabilities add up. Continue.\")\n",
    "\n",
    "# Markov model function for forecasting based on days given\n",
    "def activity_forecast(days):\n",
    "    # Choose the starting state\n",
    "    activityToday = \"Rest\"\n",
    "    print(\"Start state: \" + activityToday)\n",
    "    # List to store state transitions\n",
    "    activityList = [activityToday]\n",
    "    i = 0\n",
    "    # Calculation of the probability given state transition\n",
    "    prob = 1\n",
    "    while i != days:\n",
    "        if activityToday == \"Rest\":\n",
    "            change = np.random.choice(transitionName[0],replace=True,p=transitionMatrix[0])\n",
    "            if change == \"RR\":\n",
    "                prob = prob * 0.2\n",
    "                activityList.append(\"Rest\")\n",
    "                pass\n",
    "            elif change == \"RE\":\n",
    "                prob = prob * 0.6\n",
    "                activityToday = \"Exercise\"\n",
    "                activityList.append(\"Exercise\")\n",
    "            else:\n",
    "                prob = prob * 0.2\n",
    "                activityToday = \"Sweets\"\n",
    "                activityList.append(\"Sweets\")\n",
    "        elif activityToday == \"Exercise\":\n",
    "            change = np.random.choice(transitionName[1],replace=True,p=transitionMatrix[1])\n",
    "            if change == \"EE\":\n",
    "                prob = prob * 0.5\n",
    "                activityList.append(\"Exercise\")\n",
    "                pass\n",
    "            elif change == \"ER\":\n",
    "                prob = prob * 0.2\n",
    "                activityToday = \"Rest\"\n",
    "                activityList.append(\"Rest\")\n",
    "            else:\n",
    "                prob = prob * 0.3\n",
    "                activityToday = \"Sweets\"\n",
    "                activityList.append(\"Sweets\")\n",
    "        elif activityToday == \"Sweets\":\n",
    "            change = np.random.choice(transitionName[2],replace=True,p=transitionMatrix[2])\n",
    "            if change == \"SS\":\n",
    "                prob = prob * 0.1\n",
    "                activityList.append(\"Sweets\")\n",
    "                pass\n",
    "            elif change == \"SR\":\n",
    "                prob = prob * 0.2\n",
    "                activityToday = \"Rest\"\n",
    "                activityList.append(\"Rest\")\n",
    "            else:\n",
    "                prob = prob * 0.7\n",
    "                activityToday = \"Exercise\"\n",
    "                activityList.append(\"Exercise\")\n",
    "        i += 1  \n",
    "    print(\"Possible states: \" + str(activityList))\n",
    "    print(\"End state after \"+ str(days) + \" days: \" + activityToday)\n",
    "    print(\"Probability of the possible sequence of states: \" + str(prob))\n",
    "\n",
    "# Function to forecast state based on number of days parameter\n",
    "activity_forecast(15)"
   ]
